<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
<mapper namespace="cn.tju.vo.mapping.SampleCNS">
	<insert id="doCreate" parameterType="SampleC" keyProperty="geneId" useGeneratedKeys="true">
		INSERT INTO sample_c(cancerName,sampleIdN,sampleIdT)
		VALUES (#{cancerName},#{sampleIdN},#{sampleIdT}) ;
	</insert>
</mapper>